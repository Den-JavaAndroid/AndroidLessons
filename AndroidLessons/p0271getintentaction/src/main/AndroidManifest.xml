<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comdeveloper_megamozg.wix.jaddev888.getintentaction" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Info" android:label="Data/time" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"></category>
                <action android:name="ru.startandroid.intent.action.showtime"></action>
                <action android:name="ru.startandroid.intent.action.showdate"></action>
            </intent-filter>
        </activity>
    </application>

</manifest>
